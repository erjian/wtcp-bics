{
  "wtcp-bic-hall": {
    "settings": {
      "index": {
        "number_of_shards": "5",
        "number_of_replicas": "1"
      }
    }
  },
  "mappings": {
    "resource": {
      "dynamic": "false",
      "properties": {
        "id": {
          "type": "keyword",
          "ignore_above": 256
        },
        "code": {
          "type": "keyword",
          "ignore_above": 256
        },
        "title": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "fullSpell": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "simpleSpell": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "summary": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "description": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "venueId": {
          "type": "keyword",
          "ignore_above": 256
        },
        "category": {
          "type": "keyword",
          "ignore_above": 256
        },
        "area": {
          "type": "integer"
        },
        "maxReceptionNum": {
          "type": "integer"
        },
        "weight": {
          "type": "integer"
        },
        "longitude": {
          "type": "keyword",
          "ignore_above": 256
        },
        "latitude": {
          "type": "keyword",
          "ignore_above": 256
        },
        "geoPoint": {
          "type": "geo_point",
          "doc_values": true
        },
        "vrUrl": {
          "type": "keyword",
          "ignore_above": 256
        },
        "images": {
          "type": "keyword",
          "ignore_above": 256
        },
        "vrImageUrl": {
          "type": "keyword",
          "ignore_above": 256
        },
        "videoResources": {
          "type": "keyword",
          "ignore_above": 256
        },
        "videos": {
          "type": "keyword",
          "ignore_above": 256
        },
        "audios": {
          "type": "keyword",
          "ignore_above": 256
        },
        "tags": {
          "type": "keyword",
          "ignore_above": 256,
          "copy_to": "allTags"
        },
        "relateTags": {
          "type": "keyword",
          "ignore_above": 256,
          "copy_to": "allTags"
        },
        "allTags": {
          "type": "keyword",
          "ignore_above": 256
        },
        "publishStatus": {
          "type": "integer"
        },
        "deptCode": {
          "type": "keyword",
          "ignore_above": 256
        }
      }
    }
  }
}