{
  "wtcp-bic-rural": {
    "settings": {
      "index": {
        "number_of_shards": "5",
        "number_of_replicas": "1"
      }
    }
  },
  "mappings": {
    "resource": {
      "dynamic": "false",
      "properties": {
        "id": {
          "type": "keyword",
          "ignore_above": 256
        },
        "code": {
          "type": "keyword",
          "ignore_above": 256
        },
        "title": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "subTitle": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "slogan": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "summary": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "description": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "address": {
          "type": "keyword",
          "ignore_above": 256
        },
        "serviceFacility": {
          "type": "keyword",
          "ignore_above": 256
        },
        "type": {
          "type": "keyword",
          "ignore_above": 256
        },
        "weight": {
          "type": "long"
        },
        "longitude": {
          "type": "keyword",
          "ignore_above": 256
        },
        "latitude": {
          "type": "keyword",
          "ignore_above": 256
        },
        "geoPoint": {
          "type": "geo_point",
          "doc_values": true
        },
        "region": {
          "type": "keyword",
          "ignore_above": 256
        },
        "regionFullCode": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "regionFullName": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          },
          "analyzer": "ik_max_word"
        },
        "withinScenic": {
          "type": "integer"
        },
        "scenicId": {
          "type": "keyword",
          "ignore_above": 256
        },
        "withinPark": {
          "type": "integer"
        },
        "num": {
          "type": "integer"
        },
        "images": {
          "type": "keyword",
          "ignore_above": 256
        },
        "videos": {
          "type": "keyword",
          "ignore_above": 256
        },
        "audios": {
          "type": "keyword",
          "ignore_above": 256
        },
        "tags": {
          "type": "keyword",
          "ignore_above": 256,
          "copy_to": "allTags"
        },
        "relateTags": {
          "type": "keyword",
          "ignore_above": 256,
          "copy_to": "allTags"
        },
        "allTags": {
          "type": "keyword",
          "ignore_above": 256
        },
        "publishStatus": {
          "type": "integer"
        },
        "deptCode": {
          "type": "keyword",
          "ignore_above": 256
        }
      }
    }
  }
}